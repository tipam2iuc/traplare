<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="100dp"
    android:id="@+id/layout_template_travel2"
    android:paddingLeft="10dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="wrap_content"
    android:background="#fefefe"
    android:layout_marginLeft="8dp"
    tools:context=".PlanningActivity"

    xmlns:android="http://schemas.android.com/apk/res/android">

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageCicle_main_logo2"
        android:src="@drawable/ic_logo_mini"
        android:layout_width="15dp"
        android:layout_marginRight="15dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="15dp"
        android:layout_height="15dp">
    </de.hdodenhof.circleimageview.CircleImageView>

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageView_main_container"
        android:src="@drawable/backgroundblue2"
        android:layout_width="50dp"
        app:layout_constraintLeft_toRightOf="@id/textView_agency_from"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginRight="30dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_height="50dp">
    </de.hdodenhof.circleimageview.CircleImageView>

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageView_main_detail"
        android:src="@drawable/question_detail"
        android:layout_width="25dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_height="25dp">
    </de.hdodenhof.circleimageview.CircleImageView>

    <TextView
        android:id="@+id/textView_agency_title"
        android:layout_width="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/textView_agency_from"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="5dp"
        android:text="Agency : "
        android:textSize="9dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView_agency_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="30dp"
        android:text="Agency value"
        android:textSize="9dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@id/textView_agency_from"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView_agency_from"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_agency_from"
        android:layout_width="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/textView_agency_provenance_value"
        app:layout_constraintTop_toBottomOf="@id/textView_agency_title"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="10dp"
        android:text="Provenance : "
        android:textSize="8dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView_agency_provenance_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:text="provenance value"
        android:textSize="11dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@id/textView_agency_destination"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView_agency_from"
        tools:layout_editor_absoluteY="37dp" />

    <TextView
        android:id="@+id/textView_agency_destination"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/textView_agency_provenance_value"
        app:layout_constraintBottom_toTopOf="@id/textView_agency_destination_value"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="10dp"
        android:text="Destination : "
        android:textSize="8dp"
        android:textStyle="bold"
        tools:ignore="DuplicateIds" />

    <TextView
        android:id="@+id/textView_agency_destination_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Destination value"
        android:textSize="11dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textStyle="bold"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toBottomOf="@id/textView_agency_destination"
    />
</android.support.constraint.ConstraintLayout>