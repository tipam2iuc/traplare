<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="110dp"
    android:id="@+id/layout_template_travel"
    android:paddingTop="10dp"
    android:paddingLeft="10dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_marginTop="5dp"
    android:layout_marginHorizontal="10dp"
    android:background="@drawable/bg_trip"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageCicle_main_logo"
        android:src="@drawable/bluebackground1"
        android:layout_width="30dp"
        android:layout_marginRight="15dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_height="30dp">
    </de.hdodenhof.circleimageview.CircleImageView>

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageCicle_main_logo2"
        android:src="@drawable/bluebackground1"
        android:layout_width="15dp"
        android:layout_marginRight="15dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="15dp"
        android:layout_height="15dp">
    </de.hdodenhof.circleimageview.CircleImageView>

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageView_main_container"
        android:src="@drawable/backgroundblue2"
        android:layout_width="70dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginRight="30dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_height="50dp">
    </de.hdodenhof.circleimageview.CircleImageView>

    <TextView
        android:id="@+id/textView_agency_title"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="10dp"
        android:text="Agency : "
        android:textSize="11dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView_agency_value"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@id/textView_agency_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="10dp"
        android:text="Agency value"
        android:textSize="11dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView_agency_from"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/textView_agency_title"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="10dp"
        android:text="Provenance : "
        android:textSize="11dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView_agency_provenance_value"
        app:layout_constraintTop_toBottomOf="@id/textView_agency_value"
        app:layout_constraintLeft_toRightOf="@id/textView_agency_from"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="10dp"
        android:text="provenance value"
        android:textSize="11dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView_agency_destination"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/textView_agency_from"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="10dp"
        android:text="Destination : "
        android:textSize="11dp"
        android:textStyle="bold"
        tools:ignore="DuplicateIds" />

    <TextView
        android:id="@+id/textView_agency_destination_value"
        app:layout_constraintTop_toBottomOf="@id/textView_agency_provenance_value"
        app:layout_constraintLeft_toRightOf="@id/textView_agency_destination"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="10dp"
        android:text="Destination value"
        android:textSize="11dp"
        android:textStyle="bold" />

</android.support.constraint.ConstraintLayout>